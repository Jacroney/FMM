# ============================================================================
# PLAID CONFIGURATION - Frontend (.env)
# ============================================================================
# Copy these to your .env file and fill in your Plaid credentials

# Plaid Client ID (get from Plaid Dashboard)
VITE_PLAID_CLIENT_ID=your_plaid_client_id_here

# Plaid Environment: sandbox | development | production
VITE_PLAID_ENV=sandbox


# ============================================================================
# PLAID CONFIGURATION - Supabase Edge Functions
# ============================================================================
# Add these to your Supabase Edge Function secrets
# Run: npx supabase secrets set PLAID_CLIENT_ID=your_client_id

PLAID_CLIENT_ID=your_plaid_client_id_here
PLAID_SECRET=your_plaid_secret_here
PLAID_ENV=sandbox
